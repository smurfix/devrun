config:
  amqp:
    server:
      host: localhost
      login: test
      password: test
      virtualhost: /test
    handlers:
      dead: 1
  etcd:
    host: localhost
    port: 2379
    root: /test/evc
devices:
  test:
    what:
      type: ping
      config:
        interval: 0.2
    one:
      type: pling
      doc: one slave
      config:
        delay: 1
        want: two
    two:
      type: pling
      doc: master
      config:
        delay: 2
    three:
      type: pling
      doc: another slave
      config:
        delay: 3
        want: two

